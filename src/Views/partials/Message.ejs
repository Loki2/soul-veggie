<% if(locals.messages) { %>
  <div class="col-lg-12 col-md-12 col-sm-12 mb-0">
    <div class="pd-20 card-box height-100-p">
      <% Object.keys(messages).forEach((key) => { %>
        <% messages[key].forEach((message) => { %>
          <div class="alert <%= key %>" role="alert">
            <%= message %>
          </div>
        <% });%>
      <% });%>
    </div>
  </div>
<% } %>